@model stloumustdo.Models.StatewideOutdoors

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>StatewideOutdoors</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="OutdoorName" class="control-label"></label>
                <input asp-for="OutdoorName" class="form-control" />
                <span asp-validation-for="OutdoorName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DistanceFromSTL" class="control-label"></label>
                <input asp-for="DistanceFromSTL" class="form-control" />
                <span asp-validation-for="DistanceFromSTL" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OutdoorUrl" class="control-label"></label>
                <input asp-for="OutdoorUrl" class="form-control" />
                <span asp-validation-for="OutdoorUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Address" class="control-label"></label>
                <input asp-for="Address" class="form-control" />
                <span asp-validation-for="Address" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
